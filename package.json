{
	"private": true,
	"scripts": {
	  "build": "yarn run wp-scripts-build",
	  "start": "yarn run wp-scripts-start",
	  "prod": "yarn run build",
	  "reinstall": "rm node_modules -rf; yarn install",
	  "webpack-production": "cross-env NODE_ENV=production webpack --mode=production --config build/webpack.config.js",
	  "webpack-development": "cross-env NODE_ENV=development webpack --mode=development --mode=development --config build/webpack.config.js",
	  "wp-scripts-start": "wp-scripts start --config ./build/webpack.config.js",
	  "wp-scripts-build": "wp-scripts build --config ./build/webpack.config.js"
	},
